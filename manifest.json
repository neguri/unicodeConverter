{
    "name": "unicode to Korean",
    "version": "0.0.0.1",
    "manifest_version": 2,
    "description": "unicode to Korean.",
    "browser_action": {
        "default_popup": "popup/popup.html"
    },
    "content_scripts": [
        {
            "matches": [
              "<all_urls>"
            ],
            "js": ["popup/popup.js"]
        }
    ] 
}